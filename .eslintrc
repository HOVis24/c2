{
  "extends": ["@nuxtjs/eslint-config-typescript"],
  "rules": {
    "@typescript-eslint/no-unused-vars": ["off"],
    "func-call-spacing": "off",
    "vue/multi-word-component-names": "off",
    "comma-dangle": "off",
    "new-cap": "warn",
    "arrow-parens": "off",
    "space-before-function-paren": "off",
    "curly": "off"
  }
}
